from Coding.Strings import reverseString, reverseString_TwoPointer,  reverseString_ByReversed, reverseString,\
    reversePartOfString

print(reverseString("Shweta"))
print(reverseString_TwoPointer("Shweta"))
print(reverseString_ByReversed("Shweta"))
print(reversePartOfString("Shweta",1,4))