package ProblemStatements;

public class MyQueue<T> {
	
	private T[] data = null;
	private int front = -1;
	private int rear = -1;
	private int size = 10;
	
	public MyQueue(){
		data = (T[]) new Object[this.size];
	}
	
	public MyQueue(int size){
		this.size=size;
		data = (T[]) new Object[this.size];
	}
	
}
